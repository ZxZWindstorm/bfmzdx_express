
.log_view image{width: 120rpx !important; height: 120rpx !important; border-radius: 50%; margin-right: 20rpx;
					border: 10rpx solid #999999;}
text{display: block; margin: 10rpx 0; color: #999999;}
.log_view{display: -webkit-box;display: -webkit-flex;display: flex; -webkit-box-align: center; -webkit-align-items: center; align-items: center; padding: 0 30rpx; -webkit-align-content: center; align-content: center;
			height: 200rpx;}
.log_text text{font-size: 35rpx;}
.home{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	background:-webkit-linear-gradient(bottom, #ffe566 10%, #ffd300 100%);
	background:linear-gradient(to top, #ffe566 10%, #ffd300 100%);
	width: 100%;
	height: 400rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.noLog_view{
	margin: 0 auto;
}
.noLog_view_text{
	font-size: 38rpx;
	color: #ffffff;
	margin-bottom: 25rpx;
}
.noLog_button{
	font-size: 35rpx; color: #FFFFFF; margin-bottom: 25rpx;
}
.noLog_view button{
	border: none;
	font-size: 30rpx; 
	background: -webkit-linear-gradient(left, #28a6f1 10%, #0e8dd7 80%); 
