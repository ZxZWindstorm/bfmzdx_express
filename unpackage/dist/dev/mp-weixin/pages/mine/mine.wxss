.home {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 300rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.noLog_view {
	margin: 0 auto;
	/* height: 300rpx; */
}
.noLog_view_text {
	font-size: 38rpx;
